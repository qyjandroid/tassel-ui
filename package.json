{
    "name": "tassel-ui-components",
    "version": "2.0.4",
    "description": "preact 公共组件UI库",
    "main": "lib/index.js",
    "files": [
        "lib",
        "env",
        "src",
        "esm"
    ],
    "typings": "lib/index.d.ts",
    "module": "esm/index.js",
    "scripts": {
        "test": "jest --watch",
        "test:once": "jest",
        "pub": "npm run build && npm publish",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
        "clean": "rimraf lib esm dist",
        "build": "npm run clean && npm run build:types && gulp",
        "release": "node ./scripts/release.js",
        "new": "plop --plopfile ./scripts/plopfile.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qyjandroid/tassel-ui.git"
    },
    "sideEffects": [
        "dist/*",
        "esm/**/style/*",
        "lib/**/style/*"
    ],
    "keywords": [
        "components",
        "ui",
        "preact",
        "preact-component",
        "权跃杰",
        "quanyj"
    ],
